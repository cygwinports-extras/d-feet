inherit gnome2 python

NAME="d-feet"
VERSION=0.3.13
RELEASE=1
CATEGORY="Devel"
SUMMARY="D-Bus debugger"
DESCRIPTION="D-Bus is a messaging library used on the Desktop. D-Feet can be
used to inspect D-Bus interfaces of running programs and invoke methods on
those interfaces."
